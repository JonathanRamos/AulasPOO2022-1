import java.util.Date;
public class Pessoa {
	private String nome;
	private String CPF;
	private Date dataNascimento;

	public Pessoa(String _nome, String CPF, Date data) {
		this.setNome(_nome);
		this.setCPF(CPF);
		this.dataNascimento = data;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getCPF() {
		return CPF;
	}

	public void setCPF(String cPF) {
		CPF = cPF;
	}
	
	// Getters and Setters
}
