
public class Main {
	
	public static void main(String []args) {
		Pessoa p = new Pessoa();
		p.setNome("Jonathan");
		p.setIdade(18);
		p.setSalarioMensal(500.00);
		
		
		System.out.println("Nome: " + p.getNome() +
				" Idade: " + p.getIdade() + 
				" Salario Mensal: " + p.getSalarioMensal() + 
				" Salario Anual: " + p.getSalarioAnual());
		
		System.out.println(p.toString());
	}

}
