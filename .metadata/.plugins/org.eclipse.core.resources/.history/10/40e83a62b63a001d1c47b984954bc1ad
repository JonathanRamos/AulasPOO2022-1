import java.util.Date;
public class Pessoa {
	private String nome;
	private String CPF;
	private Date dataNascimento;

	public Pessoa(String _nome, String CPF, Date data) {
		this.setNome(_nome);
		this.CPF = CPF;
		this.dataNascimento = data;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}
	
	// Getters and Setters
}
