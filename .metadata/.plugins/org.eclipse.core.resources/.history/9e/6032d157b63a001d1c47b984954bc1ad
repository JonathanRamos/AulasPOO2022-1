import java.util.Date;
public class Pessoa {
	private String nome;
	private String cpf;
	private Date data_nascimento;

	public Pessoa(String _nome, String _cpf, Date _data) {
		this.setNome(_nome);
		this.cpf = _cpf;
		this.data_nascimento = _data;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}
	
	// Getters and Setters
}
